<div role="tabpanel" class="tab-pane active" id="profile">
  <div class="row">
    <div class="table-responsive">
      <table class="table table-bordered table-hover table-striped">
        <thead>
          <tr>
            <th>Email</th>
            <th>Phone</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td> <%= @user.email %></td>
            <td> <%= @user.phone %></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
    <div class="row">
        <div class="col-lg-12">
            <h3 class="page-header">Scores</h3>
        </div>
    </div>
  <div class="row">
    <div class="table-responsive">
      <table class="table table-bordered table-hover table-striped">
        <thead>
          <tr>
            <th>ACT Score</th>
            <th>SAT Score</th>
            <th>SAT Math</th>
            <th>PSAT English</th>
          </tr>
        </thead>
        <tbody>
          <% @user.scores.each do |score| %>
          <tr>
            <td> <%= score.act %></td>
            <td> <%= score.sat %></td>
            <td> <%= score.sat_math %></td>
            <td> <%= score.sat %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
    <div class="row">
        <div class="col-lg-12">
            <h3 class="page-header">Practice Scores</h3>
        </div>
    </div>
  <div class="row">
    <div class="table-responsive">
      <table class="table table-bordered table-hover table-striped">
        <thead>
          <tr>
            <th>PSAT Math</th>
            <th>SAT</th>
            <th>SAT Math</th>
            <th>SAT English</th>
          </tr>
        </thead>
        <tbody>
          <% @user.scores.each do |score| %>
          <tr>
            <td> <%= score.sat %></td>
            <td> <%= score.act %></td>
            <td> <%= score.sat_math %></td>
            <td> <%= score.sat_english %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="row">
      <div class="col-lg-12">
          <h3 class="page-header">Colleges</h3>
      </div>
  </div>
<div class="row">
  <div class="table-responsive">
    <table class="table table-bordered table-hover table-striped">
      <thead>
        <tr>
          <th>College Name</th>
          <th>SAT Scores Needed</th>
          <th>PSAT Scored Needed</th>
          <th>Applied?</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td> Dummy Data</td>
          <td> Dummy Data</td>
          <td> Dummy Data</td>
          <td> Dummy Data</td>
        </tr>
        <tr>
          <td> Dummy Data</td>
          <td> Dummy Data</td>
          <td> Dummy Data</td>
          <td> Dummy Data</td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
</div>
