<div role="tabpanel" class="tab-pane active" id="profile">
    <div class="row">
        <div class="col-lg-12">
            <h3 class="page-header">Scores</h3>
        </div>
    </div>
  <div class="row">
    <div class="table-responsive">
      <div class="table-responsive" style="margin-top: 10px;">
        <table class="table table-bordered table-hover table-striped">
          <thead>
            <tr>
              <th colspan="4">Sat Scores</th>
            </tr>
            <tr>
              <th>Sat Math</th>
              <th>SAT Englth</th>
              <th>SAT Date</th>
              <th>Delete</th>
            </tr>
          </thead>
          <tbody>
            <% @student.sat_scores.each do |score| %>
            <tr>
              <td> <%= score.math %></td>
              <td> <%= score.english %></td>
              <td> <%= score.date %></td>
              <td><%= link_to "", sat_score_path(score), method: :put, :class=> "fa fa-trash-o fa-lg" %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <div class="table-responsive" style="margin-top: 10px;">
        <table class="table table-bordered table-hover table-striped">
          <thead>
            <tr>
              <th colspan="6">ACT Scores</th>
            </tr>
            <tr>
              <th>ACT Math</th>
              <th>ACT English</th>
              <th>ACT Science</th>
              <th>ACT Reading</th>
              <th>ACT Date</th>
              <th>Delete</th>
            </tr>
          </thead>
          <tbody>
            <% @student.act_scores.each do |score| %>
