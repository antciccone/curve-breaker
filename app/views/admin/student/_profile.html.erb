              <td> <%= score.date %></td>
              <td><%= link_to "", sat_score_path(score), method: :put, :class=> "fa fa-trash-o fa-lg" %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <div class="table-responsive" style="margin-top: 10px;">
        <table class="table table-bordered table-hover table-striped">
          <thead>
            <tr>
              <th colspan="6">Sat Practice Scores</th>
            </tr>
            <tr>
              <th>SAT Practice Math</th>
              <th>SAT Practice Englth</th>
              <th>SAT Practice Date</th>
              <th>Delete</th>
            </tr>
          </thead>
          <tbody>
            <% @student.sat_practices.each do |score| %>
            <tr>
              <td> <%= score.math %></td>
              <td> <%= score.english %></td>
              <td> <%= score.date %></td>
              <td><%= link_to "", sat_score_path(score), method: :put, :class=> "fa fa-trash-o fa-lg" %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
